<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".estudante_activities._a.activities.EstPresenca">

    <android.support.design.widget.TabLayout
        android:id="@+id/est_pres_tab_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:tabTextColor="#61b0c1"
        app:tabSelectedTextColor="#fff"
        android:background="@color/colorPrimary"
        app:tabMode="fixed"
        app:tabGravity="fill"
        app:tabIndicatorColor="#fff" >

    </android.support.design.widget.TabLayout>

    <com.example.root.re_presencas.all.adapters.ViewPagerSwipe
        android:id="@+id/est_pres_view_pager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/est_pres_tab_layout">

    </com.example.root.re_presencas.all.adapters.ViewPagerSwipe>

</android.support.constraint.ConstraintLayout>